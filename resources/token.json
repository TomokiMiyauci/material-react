{
  "md": {
    "ref": {
      "typeface": {
        "brand": {
          "$type": "fontFamily",
          "$value": "Roboto",
          "$description": "Brand typeface"
        },
        "plain": {
          "$type": "fontFamily",
          "$value": "Roboto",
          "$description": "Plain typeface"
        },
        "weight": {
          "regular": {
            "$type": "fontWeight",
            "$value": 400,
            "$description": "Regular weight"
          },
          "medium": {
            "$type": "fontWeight",
            "$value": 500,
            "$description": "Medium weight"
          },
          "bold": {
            "$type": "fontWeight",
            "$value": 700,
            "$description": "Bold weight"
          }
        }
      }
    },
    "sys": {
      "shape": {
        "corner": {
          "full": {
            "$type": "dimension",
            "$value": { "value": 9999, "unit": "px" },
            "$description": "Fully rounded"
          },
          "extra large": {
            "$type": "dimension",
            "$value": { "value": 28, "unit": "dp" },
            "$description": "Extra large rounding"
          },
          "large": {
            "$type": "dimension",
            "$value": { "value": 16, "unit": "dp" },
            "$description": "Large rounding"
          },
          "medium": {
            "$type": "dimension",
            "$value": { "value": 12, "unit": "dp" },
            "$description": "Medium rounding"
          },
          "small": {
            "$type": "dimension",
            "$value": { "value": 8, "unit": "dp" },
            "$description": "Small rounding"
          },
          "extra small": {
            "$type": "dimension",
            "$value": { "value": 4, "unit": "dp" },
            "$description": "Extra small rounding"
          },
          "none": {
            "$type": "number",
            "$value": 0,
            "$description": "No rounding"
          }
        }
      },
      "typescale": {
        "display": {
          "large": {
            "font": {
              "$value": "{md.ref.typeface.brand}",
              "$description": "Display Large font name"
            },
            "weight": {
              "$value": "{md.ref.typeface.weight.regular}",
              "$description": "Display Large font weight"
            },
            "size": {
              "$type": "dimension",
              "$value": { "value": 57, "unit": "dp" },
              "$description": "Display Large font size"
            },
            "tracking": {
              "$type": "dimension",
              "$value": { "value": -0.25, "unit": "dp" },
              "$description": "Display Large font tracking"
            },
            "line height": {
              "$type": "dimension",
              "$value": { "value": 64, "unit": "dp" },
              "$description": "Display Large line height"
            }
          }
        }
      },
      "state": {
        "dragged": {
          "state layer": {
            "opacity": {
              "$type": "number",
              "$value": 0.16,
              "$description": "Dragged state layer opacity"
            }
          }
        },
        "pressed": {
          "state layer": {
            "opacity": {
              "$type": "number",
              "$value": 0.1,
              "$description": "Pressed state layer opacity"
            }
          }
        },
        "focus": {
          "state layer": {
            "opacity": {
              "$type": "number",
              "$value": 0.1,
              "$description": "Focus state layer opacity"
            }
          }
        },
        "hover": {
          "state layer": {
            "opacity": {
              "$type": "number",
              "$value": 0.08,
              "$description": "Hover state layer opacity"
            }
          }
        }
      }
    }
  }
}
