{
  "container": {
    "elevation": {
      "$type": "token",
      "$value": "md.sys.elevation.level2",
      "$description": "Nav bar container elevation"
    },
    "shadow color": {
      "$type": "token",
      "$value": "md.sys.color.shadow",
      "$description": "Nav bar container shadow color"
    },
    "color": {
      "$type": "token",
      "$value": "md.sys.color.surface-container",
      "$description": "Nav bar container color"
    },
    "height": {
      "$type": "dimension",
      "$value": { "value": 64, "unit": "dp" },
      "$description": "Nav bar height"
    },
    "shape": {
      "$type": "token",
      "$value": "md.sys.shape.corner.none",
      "$description": "Nav bar shape"
    },
    "items space": {
      "$type": "dimension",
      "$value": { "value": 0, "unit": "dp" },
      "$description": "Nav bar height"
    },
    "vertical": {
      "between space": {
        "$type": "dimension",
        "$value": { "value": 6, "unit": "dp" },
        "$description": "Nav bar item vertical container between space"
      }
    }
  },
  "indicator": {
    "color": {
      "$type": "token",
      "$value": "md.sys.color.secondary-container",
      "$description": "Nav bar item indicator color"
    },
    "vertical": {
      "active": {
        "height": {
          "$type": "dimension",
          "$value": { "value": 32, "unit": "dp" },
          "$description": "Nav bar item vertical active indicator height"
        },
        "width": {
          "$type": "dimension",
          "$value": { "value": 56, "unit": "dp" },
          "$description": "Nav bar item vertical active indicator width"
        }
      }
    }
  },
  "item": {
    "shape": {
      "$type": "token",
      "$value": "md.sys.shape.corner.full",
      "$description": "Nav bar item shape"
    },
    "space between icon label": {
      "space": {
        "$type": "dimension",
        "$value": { "value": 4, "unit": "dp" },
        "$description": "Nav bar item space between icon label space"
      }
    },
    "icon": {
      "size": {
        "$type": "dimension",
        "$value": { "value": 24, "unit": "dp" },
        "$description": "Nav bar item icon size"
      },
      "active": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.on-secondary-container",
          "$description": "Nav bar item active icon color"
        }
      },
      "inactive": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.on-surface-variant",
          "$description": "Nav bar item inactive icon color"
        }
      }
    },
    "vertical": {
      "space": {
        "$type": "dimension",
        "$value": { "value": 4, "unit": "dp" },
        "$description": "Nav bar item vertical active indicator icon label space"
      }
    },
    "label text": {
      "active": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.secondary",
          "$description": "Nav bar item active label text color"
        }
      },
      "inactive": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.on-surface-variant",
          "$description": "Nav bar item inactive label text color"
        }
      },
      "vertical": {
        "font": {
          "$type": "token",
          "$value": "md.ref.typeface.plain",
          "$description": "Nav bar item vertical label text"
        },
        "line height": {
          "$type": "dimension",
          "$value": { "value": 16, "unit": "dp" },
          "$description": "Nav bar item vertical label text"
        },
        "size": {
          "$type": "dimension",
          "$value": { "value": 12, "unit": "dp" },
          "$description": "Nav bar item vertical label text"
        },
        "tracking": {
          "$type": "dimension",
          "$value": { "value": 0.5, "unit": "dp" },
          "$description": "Nav bar item vertical label text"
        },
        "weight": {
          "$type": "token",
          "$value": "md.ref.typeface.weight-medium",
          "$description": "Nav bar item vertical label text"
        }
      },
      "horizontal": {
      }
    }
  },
  "state layer": {
    "hovered": {
      "active": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.on-secondary-container",
          "$description": "Nav bar item active hovered state layer color"
        },
        "opacity": {
          "$type": "token",
          "$value": "md.sys.state.hover.state-layer-opacity",
          "$description": "Nav bar item active hovered state layer opacity"
        }
      },
      "inactive": {
        "color": {
          "$type": "token",
          "$value": "md.sys.color.on-secondary-container",
          "$description": "Nav bar item inactive hovered state layer color"
        }
      }
    }
  }
}
